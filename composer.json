{
    "name": "dejw-cake/admin-bake-theme",
    "description": "Bake plugin for CakePHP to scaffold modules",
    "type": "cakephp-plugin",
    "keywords": [
        "generator",
        "bake",
        "make",
        "crud",
        "admin",
        "cms",
        "cakephp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DejwCake",
            "email": "github@dejw.sk"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\AdminBakeTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\AdminBakeTheme\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
